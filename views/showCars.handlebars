{{#each cars}}
<p></p>
<div class="media">
  <img src="{{owner.image}}" class="mr-3" width="64px;" height="64px;">
    <div class="media-body">
        <h5 class="mt-0">{{make}} {{model}} {{year}}</h5>
        <div class="row">
            {{#each image}}
            <div class="col-sm-2">
                <img src="{{imageUrl}}" class="img-fluid" width="300px;" height="auto">
            </div>
            {{else}}
                <img src="/image/logo4.png" alt="">
            {{/each}}
        </div>
        <p>${{pricePerHour}} per hour</p>
        <p>${{pricePerWeek}} per week</p>
        <small>{{location}}</small> <br>
        <small>{{date}}</small>
        
    </div>
</div>
<hr>
{{else}}
<p>No cars found</p>
{{/each}}